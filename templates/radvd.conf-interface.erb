interface <%= name %>
{
  <%- options.sort.each do |k,v| -%>
  <%= k %> <%= v %>;
  <%- end -%>

  <%- prefixes.sort.each do |pfx,popts| -%>
  prefix <%= pfx %>
  {
    <%- popts.sort.each do |k,v| -%>
    <%= k -%> <%= v %>;
    <%- end -%>
  };
  <%- end -%>

  <%- rdnss.sort.each do |dns,popts| -%>
  RDNSS <%= dns %>
  {
    <%- popts.sort.each do |k,v| -%>
    <%= k -%> <%= v %>;
    <%- end -%>
  };
  <%- end -%>

  <%- dnssl.sort.each do |domain,popts| -%>
  DNSSL <%= domain %>
  {
    <%- popts.sort.each do |k,v| -%>
    <%= k -%> <%= v %>;
    <%- end -%>
  };
  <%- end -%>
};
